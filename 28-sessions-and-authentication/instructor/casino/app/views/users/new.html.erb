<h1>Sign Up</h1>

<%= form_for @user do |f| %>

  <div class='field'>
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>

  <div class='field'>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>

  <div class='field'>
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>   
  </div>

  <%= f.submit 'Sign Up '%>

<% end %>
